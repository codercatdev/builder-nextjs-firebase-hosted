{
  "name": "builder-next-js-simple-firebase-hosting",
  "version": "1.0.0",
  "author": {
    "name": "Alex Patterson",
    "email": "alex@codingcat.dev"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@builder.io/react": "^1.1.50",
    "@builder.io/widgets": "^1.2.21",
    "@vercel/fetch": "^6.1.0",
    "atob": "^2.1.2",
    "firebase": "^9.6.8",
    "jest": "^26.6.3",
    "next": "^12.0.0",
    "next-seo": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-merge-refs": "^1.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "next-unused": "^0.0.3",
    "prettier": "^2.1.2",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  },
  "license": "MIT"
}
